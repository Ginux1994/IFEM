<?xml version="1.0" encoding="UTF-8" standalone="yes"?>

<!-- Necking of an elasto-plastic tension rod. !-->
<!-- Single-patch axisymmetric model, 10x20 biquadratic Spline elements. !-->

<simulation>

  <geometry>
    <patchfile>strip2D1rett.g2</patchfile>
    <refine patch="1" dir="2">0.2404845</refine>
    <raiseorder patch="1" u="1" v="1"/>
    <refine patch="1" u="9" v="9"/>
    <topologysets>
      <set name="left" type="edge">
        <item patch="1">1</item>
      </set>
      <set name="bottom" type="edge">
        <item patch="1">3</item>
      </set>
      <set name="top" type="edge">
        <item patch="1">4</item>
      </set>
    </topologysets>
  </geometry>

  <boundaryconditions>
    <dirichlet comp="1" set="left"/>
    <dirichlet comp="2" set="bottom"/>
    <dirichlet comp="2" set="top" type="linear">1.0</dirichlet>
  </boundaryconditions>

  <finitedeformation>
    <formulation>
      <mixed type="Fbar"/>
      <axisymmetric/>
    </formulation>
    <plastic Emod="206.9" nu="0.29" Hiso="0.12924" yield="1.0"
             Y0="0.45" Yinf="0.715" beta="16.93" istrt="1"/>
  </finitedeformation>

  <discretization>
    <nGauss>3</nGauss>
  </discretization>

  <nonlinearsolver>
    <timestepping dtMin="0.05" dtMax="0.5">
      <step end="5.0">0.5</step>
    </timestepping>
    <rtol>1.0e-16</rtol>
  </nonlinearsolver>

  <postprocessing>
    <vtfformat nviz="3">BINARY</vtfformat>
    <resultpoints>
      <point patch="1" u="1.0" v="0.0"/>
      <point patch="1" u="0.0" v="1.0"/>
      <point patch="1" u="1.0" v="1.0"/>
    </resultpoints>
  </postprocessing>

</simulation>
